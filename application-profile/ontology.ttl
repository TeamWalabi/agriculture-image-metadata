<https://www.w3.org/ns/dcat#bbox> a owl:DatatypeProperty ;
    rdfs:domain agimage:Plot ;
    rdfs:range xsd:string ;
    rdfs:label "bbox" ;
    rdfs:comment "Plot polygon as WKT literal (dcterms:Location)" ;
