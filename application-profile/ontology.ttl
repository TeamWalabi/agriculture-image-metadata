agimage:crop a owl:ObjectProperty ;
    rdfs:domain agimage:Plot ;
    rdfs:range agimage:Crop ;
    rdfs:label "crop" ;
    rdfs:comment "List of crops planted in the plot" ;
    .

agimage:weed a owl:ObjectProperty ;
    rdfs:domain agimage:Plot ;
    rdfs:range agimage:Crop ;
    rdfs:label "weed" ;
