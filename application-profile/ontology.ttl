agimage:hasSensor a owl:ObjectProperty ;
    rdfs:domain sosa:Platform ;
    rdfs:range agimage:Sensor ;
    rdfs:label "hasSensor" ;
    rdfs:comment "List of sensors/cameras mounted on the Platform/platform" ;
    .

agimage:hasCamera a owl:ObjectProperty ;
    rdfs:domain sosa:Sensor ;
    rdfs:range agimage:Camera ;
    rdfs:label "hasCamera" ;
    .

dct:title a owl:DatatypeProperty ;
    rdfs:domain agimage:Plot ;
    rdfs:range xsd:string ;
    rdfs:label "plotName" ;
    rdfs:comment "Plot or greenhouse identifier" ;
    rdfs:comment "Example: plot123" ;
    .

dct:identifier a owl:DatatypeProperty ;
    rdfs:domain agimage:Plot ;
    rdfs:range xsd:string ;
    rdfs:label "plotID" ;
    rdfs:comment "Unique identifier for the maplot based on plot + uuid" ;
    rdfs:comment "Example: plot123_448aadb7-b07d-498d-9301-2240533d849a" ;
