@prefix aime:   <https://w3id.org/agri-image/ontology-extension#> .
@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dcterms:<http://purl.org/dc/terms/> .
@prefix exif:   <http://ns.adobe.com/exif/1.0/> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix prov:   <http://www.w3.org/ns/prov#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@prefix sosa:   <http://www.w3.org/ns/sosa/> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

# Minimal local extension layer.
# Only add terms here when no suitable term exists in an external ontology.

<https://w3id.org/agri-image/ontology-extension> a owl:Ontology ;
  dcterms:title "Agriculture Image Metadata â€“ Ontology Extension" ;
  dcterms:description "Local extension terms needed for agricultural image metadata when external ontologies do not suffice." ;
  dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
  dcterms:created "2026-02-17"^^xsd:date .

#####################################################################
# Classes (minimal; mapped to existing vocabularies where possible)
#####################################################################

aime:DatasetPackage a owl:Class ;
  rdfs:label "Dataset package"@en ;
  rdfs:subClassOf prov:Collection .

aime:Field a owl:Class ;
  rdfs:label "Field"@en ;
  rdfs:subClassOf schema:Place .

aime:Plot a owl:Class ;
  rdfs:label "Plot"@en ;
  rdfs:subClassOf schema:Place .

aime:Machine a owl:Class ;
  rdfs:label "Machine/platform"@en ;
  rdfs:subClassOf sosa:Platform .

aime:Camera a owl:Class ;
  rdfs:label "Camera"@en ;
  rdfs:subClassOf sosa:Sensor .

aime:Image a owl:Class ;
  rdfs:label "Image"@en ;
  rdfs:subClassOf schema:ImageObject .

#####################################################################
# Properties (local terms used in the JSON schema where no stable external
# property is referenced)
#####################################################################

# Dataset package containment
aime:dataset a owl:ObjectProperty ;
  rdfs:label "dataset"@en ;
  rdfs:domain aime:DatasetPackage ;
  rdfs:range dcat:Dataset ;
  rdfs:subPropertyOf dcterms:hasPart .

aime:fields a owl:ObjectProperty ;
  rdfs:label "fields"@en ;
  rdfs:domain aime:DatasetPackage ;
  rdfs:range aime:Field ;
  rdfs:subPropertyOf dcterms:hasPart .

aime:machines a owl:ObjectProperty ;
  rdfs:label "machines"@en ;
  rdfs:domain aime:DatasetPackage ;
  rdfs:range aime:Machine ;
  rdfs:subPropertyOf dcterms:hasPart .

aime:images a owl:ObjectProperty ;
  rdfs:label "images"@en ;
  rdfs:domain aime:DatasetPackage ;
  rdfs:range aime:Image ;
  rdfs:subPropertyOf dcterms:hasPart .

# Field / plot
aime:fieldName a owl:DatatypeProperty ;
  rdfs:label "fieldName"@en ;
  rdfs:domain aime:Field ;
  rdfs:range xsd:string .

aime:fieldID a owl:DatatypeProperty ;
  rdfs:label "fieldID"@en ;
  rdfs:range xsd:string .

aime:plots a owl:ObjectProperty ;
  rdfs:label "plots"@en ;
  rdfs:domain aime:Field ;
  rdfs:range aime:Plot ;
  rdfs:subPropertyOf dcterms:hasPart .

# Machine / sensors
aime:machineName a owl:DatatypeProperty ;
  rdfs:label "machineName"@en ;
  rdfs:domain aime:Machine ;
  rdfs:range xsd:string .

aime:machineID a owl:DatatypeProperty ;
  rdfs:label "machineID"@en ;
  rdfs:range xsd:string .

aime:sensors a owl:ObjectProperty ;
  rdfs:label "sensors"@en ;
  rdfs:domain aime:Machine ;
  rdfs:range aime:Camera ;
  rdfs:subPropertyOf sosa:hosts .

# Camera
aime:cameraName a owl:DatatypeProperty ;
  rdfs:label "cameraName"@en ;
  rdfs:range xsd:string .

# Image (links expressed as IDs in the schema)
aime:imageName a owl:DatatypeProperty ;
  rdfs:label "imageName"@en ;
  rdfs:range xsd:string .

aime:imageID a owl:DatatypeProperty ;
  rdfs:label "imageID"@en ;
  rdfs:range xsd:string .

aime:imageTimestamp a owl:DatatypeProperty ;
  rdfs:label "imageTimestamp"@en ;
  rdfs:range xsd:string .

aime:cameraID a owl:DatatypeProperty ;
  rdfs:label "cameraID"@en ;
  rdfs:range xsd:string ;
  rdfs:comment "Camera identifier used in data capture; may correspond to exif:SerialNumber."@en .

aime:plotID a owl:DatatypeProperty ;
  rdfs:label "plotID"@en ;
  rdfs:range xsd:string .

aime:machineIDRef a owl:DatatypeProperty ;
  rdfs:label "machineID (ref)"@en ;
  rdfs:range xsd:string ;
  rdfs:comment "Machine identifier referenced by the image record."@en .

#####################################################################
# Additional schema properties (camera/image calibration & stats)
#####################################################################

aime:ID a owl:DatatypeProperty ;
  rdfs:label "ID"@en ;
  rdfs:range xsd:string ;
  rdfs:comment "Internal UUID-style identifier used in the JSON schema."@en .

aime:numberOfImages a owl:DatatypeProperty ;
  rdfs:label "numberOfImages"@en ;
  rdfs:range xsd:integer .

aime:numberOfAnnotatedImages a owl:DatatypeProperty ;
  rdfs:label "numberOfAnnotatedImages"@en ;
  rdfs:range xsd:integer .

aime:pixelSize a owl:DatatypeProperty ;
  rdfs:label "pixelSize"@en ;
  rdfs:range xsd:decimal .

aime:spectralBand a owl:DatatypeProperty ;
  rdfs:label "spectralBand"@en ;
  rdfs:range xsd:string .

aime:cameraBox a owl:DatatypeProperty ;
  rdfs:label "cameraBox"@en ;
  rdfs:range xsd:boolean .

aime:lightModel a owl:DatatypeProperty ;
  rdfs:label "lightModel"@en ;
  rdfs:range xsd:string .

aime:intrinsics a owl:DatatypeProperty ;
  rdfs:label "intrinsics"@en ;
  rdfs:range rdf:JSON .

aime:distortionModel a owl:DatatypeProperty ;
  rdfs:label "distortionModel"@en ;
  rdfs:range xsd:string .

aime:distortionCoefficients a owl:DatatypeProperty ;
  rdfs:label "distortionCoefficients"@en ;
  rdfs:range rdf:JSON .

aime:imageXYZ a owl:DatatypeProperty ;
  rdfs:label "imageXYZ"@en ;
  rdfs:range xsd:decimal .

aime:imageQuaternionXYZW a owl:DatatypeProperty ;
  rdfs:label "imageQuaternionXYZW"@en ;
  rdfs:range xsd:decimal .

aime:imageGNSS a owl:DatatypeProperty ;
  rdfs:label "imageGNSS"@en ;
  rdfs:range xsd:string .

aime:baseXYZ a owl:DatatypeProperty ;
  rdfs:label "baseXYZ"@en ;
  rdfs:range xsd:decimal .

aime:baseQuaternionXYZW a owl:DatatypeProperty ;
  rdfs:label "baseQuaternionXYZW"@en ;
  rdfs:range xsd:decimal .

aime:baseGNSS a owl:DatatypeProperty ;
  rdfs:label "baseGNSS"@en ;
  rdfs:range xsd:string .


